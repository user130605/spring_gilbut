{{>layouts/header}}
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Email</th>
      <th scope="col">Password</th>
    </tr>
  </thead>
  <tbody>
  {{#member}}
  <tr>
      <th scope="row">{{id}}</th>
      <td colspan="2">{{email}}</td>
      <td>{{password}}</td>
  </tr>
  {{/member}}

  </tbody>
</table>

<a href="/members/{{member.id}}/edit" class="btn btn-primary">수정하기</a>
<a href="/members">Go to Article List</a>

{{>layouts/footer}}